{
  "name": "alge",
  "version": "0.0.0-dripip",
  "repository": "git@github.com:jasonkuhrt/alge.git",
  "author": "Jason Kuhrt",
  "packageManager": "yarn@3.2.1",
  "license": "MIT",
  "files": [
    "src",
    "dist"
  ],
  "type": "module",
  "exports": {
    ".": {
      "require": {
        "types": "./dist/cjs/index.d.ts",
        "default": "./dist/cjs/index.js"
      },
      "import": {
        "types": "./dist/esm/index.d.ts",
        "default": "./dist/esm/index.js"
      }
    }
  },
  "scripts": {
    "reflect:toc": "markdown-toc README.md -i --maxdepth 4 && prettier --write README.md",
    "format": "yarn format:prettier",
    "format:prettier": "prettier --write .",
    "format:check": "yarn format:check:prettier",
    "format:check:prettier": "prettier --check .",
    "lint": "eslint . --ext .ts,.tsx --fix",
    "lint:check": "eslint . --ext .ts,.tsx --max-warnings 0",
    "type-check": "yarn tsc --noEmit",
    "dev": "yarn clean && tsc --build --watch tsconfig.cjs.json tsconfig.esm.json",
    "build": "yarn clean && yarn build:cjs && yarn build:esm",
    "build:cjs": "tsc --project tsconfig.cjs.json",
    "build:esm": "tsc --project tsconfig.esm.json",
    "test": "jest",
    "clean": "rm -rf dist-cjs dist-esm node_modules/.cache",
    "release:pr": "dripip pr",
    "release:canary": "dripip preview",
    "release:stable": "dripip stable",
    "prepack": "yarn build"
  },
  "dependencies": {
    "endent": "^2.1.0",
    "remeda": "^0.0.35",
    "zod": "^3.17.3"
  },
  "devDependencies": {
    "@jest/types": "28.1.1",
    "@prisma-labs/prettier-config": "0.1.0",
    "@swc/core": "1.2.205",
    "@swc/helpers": "0.4.2",
    "@swc/jest": "0.2.21",
    "@tsconfig/node16": "1.0.3",
    "@tsconfig/recommended": "1.0.1",
    "@types/jest": "28.1.3",
    "@types/node": "17.0.39",
    "@types/semver": "7.3.10",
    "@types/semver-utils": "1.1.1",
    "@typescript-eslint/eslint-plugin": "5.29.0",
    "@typescript-eslint/parser": "5.29.0",
    "dripip": "0.10.0",
    "eslint": "8.18.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-codegen": "0.16.1",
    "eslint-plugin-deprecation": "1.3.2",
    "eslint-plugin-only-warn": "1.0.3",
    "eslint-plugin-prefer-arrow": "1.2.3",
    "eslint-plugin-simple-import-sort": "7.0.0",
    "eslint-plugin-tsdoc": "0.2.16",
    "jest": "28.1.1",
    "jest-watch-select-projects": "2.0.0",
    "jest-watch-suspend": "1.1.2",
    "jest-watch-typeahead": "1.1.0",
    "konn": "0.7.0",
    "markdown-toc": "1.2.0",
    "prettier": "2.7.1",
    "semver": "7.3.7",
    "semver-utils": "1.1.4",
    "ts-jest": "28.0.5",
    "ts-node": "10.8.1",
    "tsd": "0.21.0",
    "typescript": "4.7.4"
  }
}
